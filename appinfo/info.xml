	<version>1.0.0</version>
	<licence>agpl</licence>
	<author mail="contact@bytars.com" homepage="https://www.bytars.com">Bytars S.A.S</author>
	<namespace>BytarsSchool</namespace>
	<category>integration</category>
	<bugs>https://github.com/andresh0816/nextcloud-bytars-school-integration/issues</bugs>
	<dependencies>
		<nextcloud min-version="29" max-version="31"/>
	</dependencies>
	<types>
		<authentication/>
	</types>
	<settings>
		<admin>OCA\BytarsSchool\Settings\Admin</admin>
	</settings>
	<navigations>
		<navigation>
			<id>bytarsschool</id>
			<name>Bytars School</name>
			<route>bytarsschool.page.index</route>
			<icon>app.svg</icon>
			<type>link</type>
		</navigation>
	</navigations>
